package checkFaulls;

public class myClass {

	public static void main(String[] args) {
		Shape [] arr = {new Shape(), new Tringle(), new Circle(), new Square()};
		Shape s = new Tringle();
		Shape c = new Circle();

		System.out.println(s.getClass());
		System.out.println(c.getClass());
		System.out.println(s instanceof arr);

	}

}
