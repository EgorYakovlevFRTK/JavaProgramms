package reflectionApp;

public class Main {
	
	public static void main(String args[]) {
		EgorPomidor ep = new EgorPomidor ("Egor");
		Class cls = ep.getClass();
		Class cls2 = EgorPomidor.class;
		Class cls3 = Class.forName("reflectionApp.Main.EgorPomidor");
		EgorPomidor ep2 = (EgorPomidor) cls.newInstance();  
	}
}

class EgorPomidor {
	String s;
	
	public EgorPomidor() {
		
	}
	
	public EgorPomidor(String s) {
		this.s = s;
	}
	
	public String EgorMethod(String s) {
		System.out.println("You enter:" + s);
		return s;
	}
}